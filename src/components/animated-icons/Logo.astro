<script>
	import { animate, stagger } from "motion";

	function handleLogoAnimation() {
		animate(
			".logo-core",
			{
				rotate: [0, 10, -10, 10, 0],
				transformOrigin: "center",
			},
			{
				duration: 0.6,
				ease: "circInOut",
				delay: stagger(0.1),
			},
		);

		animate(
			".logo-letters",
			{
				scale: [1, 0.9, 1],
				fill: ["#680040", "#820050", "#680040"],
				transformOrigin: "center",
			},
			{
				duration: 0.75,
				ease: "circInOut",
			},
		);
	}

	// Initialize only once when the component is first mounted
	const logo = document.getElementById("logo");
	if (logo) {
		logo.addEventListener("mouseenter", handleLogoAnimation);
	}
</script>
